{
    "name": "sonatra/default-value-bundle",
    "type": "symfony-bundle",
    "description": "Creates or injects the default values ​​with a advanced configuration in the object",
    "keywords": ["default value", "object", "entity", "document"],
    "homepage": "https://github.com/sonatra/SonatraDefaultValueBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "François Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.7",
        "symfony/options-resolver": "~2.7",
        "symfony/property-access": "~2.7"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\DefaultValueBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
