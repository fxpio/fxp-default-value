{
    "name": "fxp/default-value",
    "type": "library",
    "description": "Creates or injects the default values ​​with a advanced configuration in the object",
    "keywords": ["default value", "object", "entity", "document"],
    "homepage": "https://github.com/fxpio/fxp-default-value",
    "license": "MIT",
    "authors": [
        {
            "name": "François Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/options-resolver": "^4.2.0",
        "symfony/property-access": "^4.2.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "^4.2.0",
        "symfony/security": "^4.2.0"
    },
    "autoload": {
        "psr-4": { "Fxp\\Component\\DefaultValue\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
