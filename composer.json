{
    "name": "sonatra/default-value",
    "type": "library",
    "description": "Creates or injects the default values ​​with a advanced configuration in the object",
    "keywords": ["default value", "object", "entity", "document"],
    "homepage": "https://github.com/sonatra/sonatra-default-value",
    "license": "MIT",
    "authors": [
        {
            "name": "François Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "symfony/options-resolver": "^3.3.0",
        "symfony/property-access": "^3.3.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "^3.3.0",
        "symfony/security": "^3.3.0"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Component\\DefaultValue\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
